<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.poinku">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Poinku"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.activity.InformasiActivity"></activity>
        <activity android:name=".view.activity.DetailTugasKhususActivity" />
        <activity android:name=".view.activity.DaftarJenisTugasKhususActivity" />
        <activity android:name=".view.activity.DaftarTugasKhususActivity" />
        <activity android:name=".view.activity.CatatActivity" />
        <activity android:name=".view.activity.MainActivity" />
        <activity android:name=".view.activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>